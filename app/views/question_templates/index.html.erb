<h1>Listing question_templates</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @question_templates.each do |question_template| %>
      <tr>
        <td><%= question_template.query %></td>
        <td><%= question_template.question%></td>
        <td><%= link_to 'Show', question_template %></td>
        <td><%= link_to 'Edit', edit_question_template_path(question_template) %></td>
        <td><%= link_to 'Destroy', question_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

